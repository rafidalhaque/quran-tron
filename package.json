{
  "name": "quran-tron",
  "productName": "AysaarQuranOffline",
  "version": "0.10.0",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --retries 2 --compilers js:babel-register --recursive --require ./test/setup.js test/**/*.spec.js",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --retries 2 --compilers js:babel-register --require ./test/setup.js ./test/e2e.js",
    "lint": "eslint --ignore-path .gitignore --format=node_modules/eslint-formatter-pretty app test *.js",
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=2096 -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.development",
    "postinstall": "concurrently \"install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "dev": "npm run hot-server -- --start-hot",
    "package": "npm run build && build --publish never",
    "pwin": "npm run build && build --win --x64",
    "plin": "npm run build && build --linux",
    "package-all": "npm run build && build -mwl",
    "cleanup": "mop -v"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "build": {
    "appId": "org.develar.ElectronReact",
    "category": "public.app-category.tools",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "main.js",
      "main.js.map",
      "package.json"
    ],
    "extraFiles": [
      "assets/"
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "nsis": {
      "installerIcon": "build/icon.ico"
    },
    "linux": {
      "target": [
        "deb"
      ],
      "fpm": [
        "--after-install=build/after-install.sh"
      ]
    }
  },
  "directories": {
    "buildResources": "resources",
    "output": "release"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chentsulin/electron-react-boilerplate.git"
  },
  "author": {
    "name": "C. T. Lin",
    "email": "chentsulin@gmail.com",
    "url": "https://github.com/chentsulin"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chentsulin/electron-react-boilerplate/issues"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "react-router",
    "flux",
    "webpack",
    "react-hot"
  ],
  "homepage": "https://aysaar.github.io/quran",
  "_homepage_linux": "Muncul di tombol website",
  "devDependencies": {
    "asar": "0.14.6",
    "babel-core": "6.26.3",
    "babel-eslint": "7.2.3",
    "babel-loader": "6.4.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-dev-expression": "0.2.3",
    "babel-plugin-tcomb": "0.3.26",
    "babel-plugin-webpack-loaders": "0.9.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-react-optimize": "1.0.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "boiler-room-custodian": "0.4.3",
    "chai": "4.3.6",
    "concurrently": "3.5.1",
    "cross-env": "7.0.3",
    "css-loader": "0.26.4",
    "devtron": "1.4.0",
    "electron": "11.5.0",
    "electron-builder": "8.7.0",
    "electron-devtools-installer": "2.2.4",
    "enzyme": "2.9.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "13.0.0",
    "eslint-formatter-pretty": "1.3.0",
    "eslint-import-resolver-webpack": "0.7.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype-errors": "1.5.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-mocha": "4.12.1",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "6.10.3",
    "express": "4.16.3",
    "extract-text-webpack-plugin": "3.0.2",
    "fbjs-scripts": "0.7.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.30.1",
    "jsdom": "9.12.0",
    "json-loader": "0.5.7",
    "minimist": "1.2.5",
    "mocha": "3.5.3",
    "react-addons-test-utils": "15.6.2",
    "redux-logger": "2.10.2",
    "sinon": "13.0.1",
    "spectron": "3.8.0",
    "style-loader": "3.3.1",
    "tcomb": "3.2.29",
    "url-loader": "0.6.2",
    "webpack": "1.15.0",
    "webpack-dev-middleware": "5.3.1",
    "webpack-hot-middleware": "2.21.2",
    "webpack-merge": "1.1.2",
    "webpack-validator": "3.0.1"
  },
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babyparse": "^0.4.6",
    "electron-debug": "^1.1.0",
    "font-awesome": "^4.7.0",
    "fs": "0.0.1-security",
    "nedb": "^1.8.0",
    "react": "^15.7.0",
    "react-dom": "^15.7.0",
    "react-native-vector-icons": "^9.0.0",
    "react-redux": "^4.4.10",
    "react-resizable-box": "^1.8.1",
    "react-router": "^6.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.4.1",
    "source-map-support": "^0.4.6"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }
}
